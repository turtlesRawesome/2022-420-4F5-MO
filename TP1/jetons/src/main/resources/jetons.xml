<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<VBox xmlns:fx="http://javafx.com/fxml"
	fx:controller="jetons.frontal.vues.VueJetons"
	styleClass="conteneur"
	id="vue-jetons">

	<ImageView>
			<Image url="@jetonLogo.png" />
	</ImageView>

	<Button
		fx:id="boutonAjouterSacAJetons"
		text="%ajouterSacAJetons"
		styleClass="gros-bouton"
		id="boutonAjouterSacAJetons">
	</Button>

	<Button
		fx:id="boutonAllerFileAttente"
		text="%allerFileAttente"
		styleClass="gros-bouton"
		id="boutonAllerFileAttente">
	</Button>
	
	
	
</VBox>